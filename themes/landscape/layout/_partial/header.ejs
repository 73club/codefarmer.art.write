<header id="header">
  <div id="header-outer">
    <div id="header-title">
      <div id="logo-wrap">
        <img id="logo-img" src="/css/images/logo.png">
      </div>
    </div>
  </div>
  <div id="header-nav">
    <nav id="main-nav">
      <a id="main-nav-toggle" class="nav-icon"></a>
      <% for (var i in theme.menu){ %>
      <a class="main-nav-link" href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
      <% } %>
    </nav>
    <nav id="sub-nav">
      <!-- <a id="nav-search-btn" class="nav-icon" title="<%= __('search') %>"></a> -->
    </nav>
  </div>
</header>